{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "sourceMaps": true,
      "protocol": "inspector",
      "name": "Debug (Custom Project Config)",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "request": "launch",
      "justMyCode": true,
      "program": "${workspaceFolder}/node_modules/ts-node-dev/lib/bin.js",
      "args": ["--inspect", "--", "${workspaceFolder}/src/index.ts"]
    }
  ]
}
