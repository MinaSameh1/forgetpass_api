{
  "name": "dashapi",
  "version": "0.0.1",
  "description": "A basic API that has basic features.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV='production' node ./build/src/index.js",
    "dev": "cross-env NODE_ENV='development' ts-node-dev --respawn ./src/index.ts",
    "debug": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "test": "cross-env NODE_ENV='test' jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=development jest --watch --detectOpenHandles",
    "test:debug": "cross-env NODE_ENV=development node --inspect ./node_modules/.bin/jest --detectOpenHandles --runInBand --watchAll=false",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.18.21",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "argon2": "^0.28.5",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "zod": "^3.17.3"
  }
}
